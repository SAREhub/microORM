{
  "name": "sarehub/microorm",
  "description": "Some stuff to support DoctrineORM in Docker env",
  "license": "MIT",
  "keywords": [
    "SARE",
    "SAREhub",
    "MySQL",
    "ORM"
  ],
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "mockery/mockery": "1.1.*",
    "sarehub/dockerutil": "0.3.*",
    "sarehub/client": "0.7.*"
  },
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "doctrine/orm": "2.6.*",
    "sarehub/commons": "0.7.*",
    "php-di/php-di": "^6.0"
  },
  "suggest": {
    "sarehub/client": "to use processors"
  },
  "autoload": {
    "psr-4": {
      "SAREhub\\": "src/SAREhub"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SAREhub\\": "tests/SAREhub"
    }
  }
}
